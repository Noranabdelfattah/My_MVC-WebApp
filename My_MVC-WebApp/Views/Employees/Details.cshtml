@model My_MVC_WebApp.Models.Employee

@{
    ViewBag.Title = "Details";
}

<h2>Employee Data </h2>

<table>
    <tr>
        <td>
            <b>Employee ID : </b>
        </td>
        <td>
            @Model.EmployeeId
        </td>
    </tr>

    <tr>
        <td>
            <b>Name : </b>
        </td>
        <td>
            @Model.Name
        </td>
    </tr>
    <tr>
        <td>
            <b>Gender: </b>
        </td>
        <td>
            @Model.Gender
        </td>
    </tr>
    <tr>
        <td>
            <b>City : </b>
        </td>
        <td>
            @Model.City
        </td>
    </tr>

</table>



<h4>@Html.ActionLink("Back To Employees List ", "Index", new { departmentId = @Model.DepartementId }) </h4>