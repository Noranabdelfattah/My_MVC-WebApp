@model  IEnumerable< My_MVC_WebApp.Models.Employee>

@using My_MVC_WebApp.Models;
@{
    ViewBag.Title = "Employee List";
}

<h2>Employee List</h2>
<div style="font-family:Arial">
    <ul>
        @foreach (Employee employee in @Model)
        {
            <li>@Html.ActionLink(employee.Name, "Details", new { @id= employee.EmployeeId })</li>
        }
    </ul>
</div>
<br /> <br />

<h4>@Html.ActionLink(" Back to Departments list ", "Index", "Departments")</h4>